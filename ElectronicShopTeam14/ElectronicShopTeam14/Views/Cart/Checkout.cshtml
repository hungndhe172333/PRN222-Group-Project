                    "Vĩnh Long": ["Vĩnh Long", "Bình Minh", "Bình Tân", "Long Hồ", "Mang Thít", "Tam Bình", "Trà Ôn", "Vũng Liêm"],
                    "Vĩnh Phúc": ["Vĩnh Yên", "Phúc Yên", "Bình Xuyên", "Lập Thạch", "Sông Lô", "Tam Đảo", "Tam Dương", "Vĩnh Tường", "Yên Lạc"],
                    "Yên Bái": ["Yên Bái", "Nghĩa Lộ", "Lục Yên", "Mù Cang Chải", "Trạm Tấu", "Trấn Yên", "Văn Chấn", "Văn Yên", "Yên Bình"]
                };


                // Lấy tham chiếu đến các phần tử
                const citySelect = document.querySelector('select[name="city"]');
                const districtInput = document.querySelector('input[name="district"]');

                // Tạo phần tử select mới cho quận/huyện
                const districtSelect = document.createElement('select');
                districtSelect.className = 'form-control';
                districtSelect.name = 'district';
                districtSelect.required = true;
                districtSelect.setAttribute('data-msg', 'Vui lòng chọn quận/huyện.');
                districtSelect.setAttribute('data-error-class', 'u-has-error');
                districtSelect.setAttribute('data-success-class', 'u-has-success');

                // Thay thế input bằng select
                districtInput.parentNode.replaceChild(districtSelect, districtInput);

                // Thêm option mặc định cho select quận/huyện
                const defaultOption = document.createElement('option');
                defaultOption.value = '';
                defaultOption.disabled = true;
                defaultOption.selected = true;
                defaultOption.textContent = 'Chọn quận/huyện';
                districtSelect.appendChild(defaultOption);

                // Hàm cập nhật danh sách quận/huyện dựa trên tỉnh/thành phố đã chọn
                function updateDistricts() {
                    // Xóa tất cả các option hiện tại (trừ option mặc định)
                    while (districtSelect.options.length > 1) {
                        districtSelect.remove(1);
                    }

                    // Lấy tỉnh/thành phố đã chọn
                    const selectedCity = citySelect.value;

                    // Nếu có dữ liệu quận/huyện cho tỉnh/thành phố đã chọn
                    if (districtsByCity[selectedCity]) {
                        // Thêm các quận/huyện vào select
                        districtsByCity[selectedCity].forEach(district => {
                            const option = document.createElement('option');
                            option.value = district;
                            option.textContent = district;
                            districtSelect.appendChild(option);
                        });

                        // Hiển thị select quận/huyện
                        districtSelect.disabled = false;
                    } else {
                        // Nếu không có dữ liệu, vô hiệu hóa select quận/huyện
                        districtSelect.disabled = true;
                    }
                }

                // Lắng nghe sự kiện thay đổi tỉnh/thành phố
                citySelect.addEventListener('change', updateDistricts);

                // Gọi hàm cập nhật khi trang được tải
                document.addEventListener('DOMContentLoaded', function () {
                    // Kiểm tra xem đã có tỉnh/thành phố được chọn chưa
                    if (citySelect.value) {
                        updateDistricts();
                    }
                });
            </script>

