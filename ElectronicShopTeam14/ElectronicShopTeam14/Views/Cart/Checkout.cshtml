                  <form class="js-validate" novalidate="novalidate" method="post" action="@Url.Action("ProcessCheckout", "Cart")">
                        <div class="row">
                            <div class="col-lg-5 order-lg-2 mb-7 mb-lg-0">
                                <div class="pl-lg-3 ">
                                    <div class="bg-gray-1 rounded-lg">
