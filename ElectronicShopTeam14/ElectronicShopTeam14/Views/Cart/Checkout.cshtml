                                                <h3 class="section-title mb-0 pb-2 font-size-25">Đơn hàng của bạn</h3>
                                            </div>
                                            <!-- End Title -->
                                            <!-- Product Content -->
                                            <table class="table">
                                                <thead>
                                                    <tr>
                                                        <th class="product-name">Sản phẩm</th>
                                                        <th class="product-size">Kích thước</th>
                                                        <th class="product-color">Màu sắc</th>
                                                        <th class="product-total">Thành tiền</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @if (Model.CartItems != null)
                                                    {
                                                        @foreach (var item in Model.CartItems)
                                                        {
                                                            <tr class="cart_item">
                                                                <td>@item.ProductName&nbsp;<strong class="product-quantity">× @item.Quantity</strong></td>
                                                                <td>@item.Size</td>
                                                                <td>@item.Color</td>
                                                                <td>@item.Total.ToString("N0") đ</td>
                                                            </tr>
                                                        }
                                                    }
                                                    else
                                                    {
                                                        <tr>
                                                            <td colspan="4" class="text-center">Không có sản phẩm nào trong giỏ hàng</td>
                                                        </tr>
                                                    }
                                                </tbody>
                                                <tfoot>
                                                    <tr>
                                                        <th>Tổng tiền</th>
                                                        <td>@Model.SubTotal.ToString("N0") đ</td>
                                                    </tr>
                                                    <tr>
                                                        <th>Phí vận chuyển</th>
                                                        <td>Miễn phí vận chuyển</td>
                                                    </tr>
                                                    <tr>
                                                        <th>Tổng cộng</th>
                                                        <td><strong>@Model.Total.ToString("N0") đ</strong></td>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                            <!-- End Product Content -->
                                            <div class="border-top border-width-3 border-color-1 pt-3 mb-3">
