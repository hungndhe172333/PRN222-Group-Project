    <script>
        function validateForm() {
            var password = document.getElementById("signupPassword").value;
            var confirmPassword = document.getElementById("signupConfirmPassword").value;
            var errorText = document.getElementById("passwordError");
            var requirementText = document.getElementById("passwordRequirement");
            var submitButton = document.getElementById("submitButton");

            var phone = document.getElementById("signupPhone").value;
            var phoneError = document.getElementById("phoneError");

            var dob = document.getElementById("signupDOB").value;
            var dobError = document.getElementById("dobError");

            var isValid = true;

            // Kiểm tra mật khẩu có ít nhất 8 ký tự và bắt đầu bằng chữ hoa
            var passwordPattern = /^[A-Z][A-Za-z0-9]{7,}$/;
            if (!passwordPattern.test(password)) {
                requirementText.classList.remove("d-none");
                isValid = false;
            } else {
                requirementText.classList.add("d-none");
            }

            // Kiểm tra mật khẩu khớp
            if (password !== confirmPassword && confirmPassword.length > 0) {
                errorText.classList.remove("d-none");
                isValid = false;
            } else {
                errorText.classList.add("d-none");
            }

            // Kiểm tra số điện thoại có đúng 10 chữ số
            var phonePattern = /^[0-9]{10}$/;
            if (!phonePattern.test(phone)) {
                phoneError.classList.remove("d-none");
                isValid = false;
            } else {
                phoneError.classList.add("d-none");
            }

            // Kiểm tra ngày sinh không quá ngày hiện tại
            var today = new Date().toISOString().split("T")[0];
            if (dob > today) {
                dobError.classList.remove("d-none");
                isValid = false;
            } else {
                dobError.classList.add("d-none");
            }

            // Cập nhật trạng thái nút đăng ký
            submitButton.disabled = !isValid;
        }
    </script>
